				<li><%= link_to "VANOS" %></li>
				<li><%= link_to "C M CONDS" %></li>
			</ul>
		</div>
		<div id="usermenu">
			<ul>
				<li><%= link_to "USUARIOS" %></li>
				<li><%= link_to "SETTINGS" %></li>
			</ul>
		</div>
	</div>

	<div id="main">
		<%= yield %>
	</div>

	<table id="endbar">
		<td><%= image_tag('CGSlogo.png', size:'72x32') %></td>
		<td>Compañía General de Servicios SRL</td>
		<td><%= image_tag('home.png', size:'32x32') %></td>
		<td>Los Zorzales 620 - Juana Koslay - San Luis</td>
		<td><%= image_tag('mail.png', size:'32x32') %></td>
		<td>G Falavigna: <%= mail_to('gfalav@hotmail.com') %> - J Pina: <%= mail_to('pinajoa@yahoo.com') %></td>
